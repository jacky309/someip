<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension point="org.eclipse.ui.handlers">
      <handler
            class="org.genivi.commonapi.someip.ui.SomeIPCommandExecutableExtensionFactory:org.genivi.commonapi.someip.ui.handler.SomeIPGenerationCommand"
            commandId="org.genivi.commonapi.someip.core.ui.handler.SomeIPGenerationCommand">
      </handler>
   </extension>
   <extension point="org.eclipse.ui.commands">
      <command
            id="org.genivi.commonapi.someip.core.ui.handler.SomeIPGenerationCommand"
            name="Generate SomeIP C++ Code">
      </command>
   </extension>
   <extension point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.genivi.commonapi.core.ui.popup.menu">
         <command
               commandId="org.genivi.commonapi.someip.core.ui.handler.SomeIPGenerationCommand"
               label="Generate SomeIP C++ Code"
               style="push">
            <visibleWhen checkEnabled="false">
               <reference definitionId="org.genivi.commonapi.core.ui.FrancaIDL.Resources.selected"></reference>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
</plugin>
